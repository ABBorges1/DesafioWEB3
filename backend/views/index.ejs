<%- include('./partials/header.ejs') %>
    <body>
        <%- include('./partials/navbar.ejs') %>
            <div class="container">
                <hr>
                <h1 id="tituloPerguntas">Zombie Boladão</h1>
                <hr>
                <table class="table table-dark table-striped">
                    <h4>Rank Geral</h4>
                    <thead>
                        <tr>
                            <th scope="col"></th>
                            <th scope="col" id="player">Player</th>
                            <th scope="col" id="score">Score</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% tables.forEach((player, index) => { %>
                            <tr>
                                <th scope="row" id="ranking"><%=index+1%>º</th>
                                <td id="nome"><%=player.nome%></td>
                                <td id="pontos"><%=player.score%></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
    </body>
    <%- include('./partials/footer.ejs') %>